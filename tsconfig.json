{
  "compilerOptions": {
    "target": "es6",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "module": "commonjs",
    "declaration": true,
    "jsx": "react",
    "importHelpers": true,
    "strictNullChecks": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": "src",
    "rootDir": ".",
    "outDir": "dist",
    "noEmit": true
  },
  "exclude": ["node_modules", "dist"]
}
